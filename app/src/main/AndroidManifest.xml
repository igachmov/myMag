<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mymag.mymag">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".model.activities.LoginActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name=".model.activities.HomeActivity" />
        <activity android:name=".model.activities.RegisterActivity" />
        <activity android:name=".model.activities.CartActivity" />

        <activity android:name=".model.activities.ProductActivity" />
        <activity android:name=".model.activities.UserActivity" />
        <activity android:name=".model.activities.CategoryActivity" />
        <activity android:name=".model.activities.ProductListActivity" />
        <activity android:name=".model.activities.CheckOutActivity"></activity>
    </application>

</manifest>